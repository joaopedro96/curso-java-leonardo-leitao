package heranca;

public class Carro {
	
	int velocidadeAtual = 0;
	
	final int velocidadeMaxima;
	
	Carro(int velocidadeMaxima) {
		this.velocidadeMaxima = velocidadeMaxima;
	}
	
	void acelerar() {
		velocidadeAtual += 5;
	}
	
	void frear() {
		if (velocidadeAtual > 0) {
			velocidadeAtual -= 5;
		}
	}

}
